<core:FragmentDefinition height="100%" width="100%" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns="sap.m">
	<Panel>
		<ObjectHeader number="{Price}" numberUnit="{ID}" title="{Name}">
			<ObjectAttribute text="{Name}"/>
			<ObjectAttribute text="{Description}"/>
		</ObjectHeader>
		<IconTabBar id="idIconTabBar" select="onDetailSelect">
			<items>
				<IconTabFilter icon="sap-icon://hint" key="Supplier" text="Supplier">
					<!--key="Detail" -->
					<content>
						<core:Fragment fragmentName="com.sap.open.offline.view.DetailInfoForm" type="XML"/>
					</content>
				</IconTabFilter>
			</items>
		</IconTabBar>
	</Panel>
</core:FragmentDefinition>